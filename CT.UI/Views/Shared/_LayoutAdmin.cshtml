    <!-- nice scroll -->
    <script src="~/Content/AdminDemo/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/AdminDemo/js/jquery.nicescroll.js" type="text/javascript"></script>
    <!-- charts scripts -->
    <script src="~/Content/AdminDemo/assets/jquery-knob/js/jquery.knob.js"></script>
    <script src="~/Content/AdminDemo/js/jquery.sparkline.js" type="text/javascript"></script>
    <script src="~/Content/AdminDemo/assets/jquery-easy-pie-chart/jquery.easy-pie-chart.js"></script>
    <script src="~/Content/AdminDemo/js/owl.carousel.js"></script>
    <!-- jQuery full calendar -->
    <
    <script src="~/Content/AdminDemo/js/fullcalendar.min.js"></script> <!-- Full Google Calendar - Calendar -->
    <script src="~/Content/AdminDemo/assets/fullcalendar/fullcalendar/fullcalendar.js"></script>
    <!--script for this page only-->
    <script src="~/Content/AdminDemo/js/calendar-custom.js"></script>
    <script src="~/Content/AdminDemo/js/jquery.rateit.min.js"></script>
    <!-- custom select -->
    <script src="~/Content/AdminDemo/js/jquery.customSelect.min.js"></script>
    <script src="~/Content/AdminDemo/assets/chart-master/Chart.js"></script>

    <!--custome script for all page-->
    <script src="~/Content/AdminDemo/js/scripts.js"></script>
    <!-- custom script for this page-->
    <script src="~/Content/AdminDemo/js/sparkline-chart.js"></script>
    <script src="~/Content/AdminDemo/js/easy-pie-chart.js"></script>
    <script src="~/Content/AdminDemo/js/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/Content/AdminDemo/js/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Content/AdminDemo/js/xcharts.min.js"></script>
    <script src="~/Content/AdminDemo/js/jquery.autosize.min.js"></script>
    <script src="~/Content/AdminDemo/js/jquery.placeholder.min.js"></script>
    <script src="~/Content/AdminDemo/js/gdp-data.js"></script>
    <script src="~/Content/AdminDemo/js/morris.min.js"></script>
    <script src="~/Content/AdminDemo/js/sparklines.js"></script>
    <script src="~/Content/AdminDemo/js/charts.js"></script>
    <script src="~/Content/AdminDemo/js/jquery.slimscroll.min.js"></script>
    <script>

        //knob
        $(function () {
            $(".knob").knob({
                'draw': function () {
                    $(this.i).val(this.cv + '%')
                }
            })
        });

        //carousel
        $(document).ready(function () {
            $("#owl-slider").owlCarousel({
                navigation: true,
                slideSpeed: 300,
                paginationSpeed: 400,
                singleItem: true

            });
        });

        //custom select box

        $(function () {
            $('select.styled').customSelect();
        });

        /* ---------- Map ---------- */
        $(function () {
            $('#map').vectorMap({
                map: 'world_mill_en',
                series: {
                    regions: [{
                        values: gdpData,
                        scale: ['#000', '#000'],
                        normalizeFunction: 'polynomial'
                    }]
                },
                backgroundColor: '#eef3f7',
                onLabelShow: function (e, el, code) {
                    el.html(el.html() + ' (GDP - ' + gdpData[code] + ')');
                }
            });
        });



    </script>

</body>
