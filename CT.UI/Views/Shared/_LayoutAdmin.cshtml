                        </ul>
                    </li>
                    @*商城管理*@
                    <li class="sub-menu">
                        <a href="javascript:;" class="">
                            <i class="icon_genius"></i>
                            <span>商城管理</span>
                            <span class="menu-arrow arrow_carrot-right"></span>
                        </a>
                        <ul class="sub">
                            <li><a class="" href="@Url.Action("Index","Goods")">商城首页</a></li>
                            <li><a class="" href="@Url.Action("Create","Goods")">添加商品</a></li>
                            <li><a class="" href="@Url.Action("Index","GoodsClass")">商品分类</a></li>
                        </ul>
                    </li>
                    @*用户管理*@
